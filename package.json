{
  "name": "light-player-vb",
  "private": false,
  "version": "0.0.37",
  "main": "./dist/light-player.umd.js",
  "module": "./dist/light-player.es.js",
  "typings": "./dist/packages/index.d.ts",
  "files": [
    "dist/*"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/light-player.es.js",
      "require": "./dist/light-player.umd.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "pub": "npm run build && cp pub-package.json dist/package.json && cd dist && npm publish",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "dependencies": {
    "highlight.js": "^11.5.1",
    "vite-plugin-md": "^0.14.0",
    "vue": "^3.2.25",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@types/node": "^17.0.38",
    "@vitejs/plugin-vue": "^2.3.3",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vite-plugin-dts": "^1.2.0",
    "vue-tsc": "^0.34.7"
  }
}
